<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:ordering="sequentially" >

    <objectAnimator
        android:duration="100"
        android:interpolator="@android:interpolator/decelerate_cubic"
        android:propertyName="pathData"
        android:valueFrom="
        M20,180
        L180,180"
        android:valueTo="
        M20,180
        L180,180"
        android:valueType="pathType" />
    <objectAnimator
        android:duration="100"
        android:interpolator="@android:interpolator/decelerate_cubic"
        android:propertyName="pathData"
        android:valueFrom="
        M20,180
        L180,180"
        android:valueTo="
        M20,90
        L180,90"
        android:valueType="pathType" />
    <objectAnimator
        android:duration="100"
        android:interpolator="@android:interpolator/decelerate_cubic"
        android:propertyName="pathData"
        android:valueFrom="
        M20,90
        L180,90"
        android:valueTo="
        M20,90
        L180,90"
        android:valueType="pathType" />
    <objectAnimator
        android:duration="100"
        android:interpolator="@android:interpolator/decelerate_cubic"
        android:propertyName="pathData"
        android:valueFrom="
        M20,90
        L180,90"
        android:valueTo="
        M20,180
        L180,180"
        android:valueType="pathType" />
</set>